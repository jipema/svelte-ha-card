{
  "name": "ha-svelte-card",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run hass",
    "dev:only": "vite --host --port 4173",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "hass:build": "IS_HASS=1 vite build --watch",
    "hass:preview": "vite preview --host --port 4173",
    "hass": "IS_HASS=true vite build  --watch & vite preview --host --port 4173"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "custom-card-helpers": "^1.9.0"
  }
}